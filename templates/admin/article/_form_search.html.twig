{{ form_start(form) }}
  <div class="row">
    <div class="col">
      {{ form_widget(form.email, {
        'attr': {
            'class': 'form-control autocomplete_input',
            'autocomplete' : 'off',
            'placeholder' : 'User email',
        }
      }) }}
    </div>
    <div class="col">
      {{ form_widget(form.status, {
        'attr': {
          'class' : 'form-control'
        }
      }) }}
    </div>
    <div class="col">
      {{ form_widget(form.dateFrom, {
        'attr': {
          'class' : 'form-control',
          'placeholder' : 'Date From',
        }
      }) }}
    </div>
    <div class="col">
      {{ form_widget(form.dateTo, {
        'attr': {
          'class' : 'form-control',
          'placeholder' : 'Date to',
        }
      }) }}
    </div>
    <div class="col">
      <button type="submit" class="btn btn-primary">Search</button>
    </div>
  </div>
{{ form_end(form) }}